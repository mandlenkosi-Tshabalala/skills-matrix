                    {
                        <tr>
                            <td>@education.Institution</td>
                            <td>@education.FieldOfStudy</td>
                            <td>@education.QualificationStartDate</td>
                            <td>@education.QualificationEndDate</td>
                            <td>@education.QualificationLevel</td>
                            <td><a @onclick="@(()=>GetEducation(education.Id))" role="button" aria-expanded="false" aria-controls="collapseExample"><i class="fas fa-edit"></i></a></td>
                            <td><a @onclick="@(()=>DeleteEducation(education.Id))" role="button" aria-expanded="false" aria-controls="collapseExample"><i class="fas fa-trash"></i></a></td>
                        </tr>
                    }

